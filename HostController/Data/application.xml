<?xml version="1.0" encoding="utf-8" ?>
<app>
  <page name="Drive" model="DrivePage" default_view="DrivePage">
    <F1 caption="NAV">
      <page name="NavApp" model="NavigationAppPage" default_view="ExternalApplicationPage">
        <F1 action="-"/>
        <F2 action="Up" behavior="PressOrHold"/>
        <F3 action="+"/>
        <F4 action="a"/>
        <F5 action="Left" behavior="PressOrHold"/>
        <F6 action="Down" behavior="PressOrHold"/>
        <F7 action="Right" behavior="PressOrHold"/>
        <F8 action="d"/>
      </page>
    </F1>
    <F2 caption="CAM">
      <page name="WebCam" model="WebCamPage" default_view="ExternalApplicationPage">
        <F1 action="Color+" behavior="PressOrHold"/>
        <F5 action="Color-" behavior="PressOrHold"/>
        <F2 action="Contrast+" behavior="PressOrHold"/>
        <F6 action="Contrast-" behavior="PressOrHold"/>
        <F3 action="Bright+" behavior="PressOrHold"/>
        <F7 action="Bright-" behavior="PressOrHold"/>
      </page>
    </F2>
    <F3 caption="WTHR">
      <page name="Weather" model="WeatherPage" default_view="WeatherPage"/>
    </F3>
    <F4 caption="TRAF">
      <page name="Traffic" model="TrafficPage" default_view="TrafficPage">
        <F1 action="Scale" caption="Change scale"/>
        <F2 action="Refresh" caption="Refresh"/>
      </page>
    </F4>
    <F5 caption="ENG">
      <page name="OBDMenu" model="PagesSelectorPage" default_view="CommonVertcalStackPage">
        <F1 caption="RPM-Load-Speed chart">
          <page name="OBDEngine1" model="OBDEngine1" default_view="OBDEngineAndFuel"/>
        </F1>
        <F2 caption="Throttle-Load-MAP chart">
          <page name="OBDEngine2" model="OBDEngine2" default_view="OBDEngineAndFuel"/>
        </F2>
        <F3 caption="Trouble codes">
          <page name="OBD_DTC" model="OBD_DTCPage" default_view="OBD_DTCPage">
            <F1 action="Refresh" caption="Refresh"/>
            <F8 action="Clear" caption="Clear errors"/>
          </page>
        </F3>
      </page>
    </F5>
    <Accept>
      <page model="MainPage" default_view="MainPage">
        <F1 caption="Power">
          <page name="Shutdown" model="ShutdownPage" default_view="CommonVertcalStackPage">
            <F1 action="Quit" caption="Quit application"/>
            <F2 action="Restart" caption="Restart system"/>
            <F3 action="Shutdown" caption="Shutdown"/>
          </page>
        </F1>
        <F2 caption="Config">
          <page name="CommonConfig" model="CommonConfigPage" default_view="CommonVertcalStackPage">
            <F1 action="ChangeLogLevel"/>
            <F2 caption="GPS and Navigation">
              <page name="ConfigGPS" model="ConfigGPSPage" default_view="CommonVertcalStackPage">
                <F1 action="ToggleGPSDaemon" />
                <F2 caption="Navit common config">
                  <page name="NavitCommonConfig" model="NavitCommonConfigPage" default_view="CommonVertcalStackPage">
                    <F1 action="ActivateGPS"/>
                    <F2 action="Autozoom"/>
                    <F3 action="Menubar"/>
                    <F4 action="Toolbar"/>
                    <F5 action="Statusbar"/>
                    <F6 action="KeepNorth"/>
                    <F7 action="LockRoad"/>
                    <F8 action="Zoom"/>
                  </page>
                </F2>
                <F3 caption="Navit OSD config">
                  <page name="NavitOSDConfig" model="NavitOSDConfigPage" default_view="CommonVertcalStackPage">
                    <F1 action="Compass"/>
                    <F2 action="ETA"/>
                    <F3 action="Navigation"/>
                    <F4 action="DistanceToNext"/>
                    <F5 action="DistanceToTarget"/>
                    <F6 action="NextTurn"/>
                  </page>
                </F3>
              </page>
            </F2>
          </page>
        </F2>
      </page>
    </Accept>
  </page>
  
  
  
  
  
  
  
  
  
  
  
  
    
    
    
    
    
    
    
    <page name="UnexpectedError" model="UnexpectedErrorModel" default_view="MultilineView">
      <Cancel name="Drive1" caption="Back"/>
    </page>
    <page name="ShutdownProgress" model="ShutdownProgressModel" default_view="MultilineView">
    </page>
    <page name="SystemInfoCatalog" model="PagesSelectorPage" default_view="CommonVertcalStackPage">
      <F1 name="LsUsb" caption="lsusb (USB info)"/>
    </page>
  </pages>
</app>
