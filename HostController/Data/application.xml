<?xml version="1.0" encoding="utf-8" ?>
<app>
  <default model="DrivePage"/>
  <pages>
    <page model="DrivePage">
      <Cancel model="MainPage" caption="Main page" behavior="Release"/>
    </page>
    <page name="MainPage">
      <Cancel model="DrivePage"/>
		</page>
    <page model="CommonConfigPage">
      <Cancel action="SaveAndReturn" caption="Return to Main Menu"/>
      <Accept model="ConfigGPSPage" view="CommonVertcalStackPage" caption="Go to GPS Config"/>
      <F1 action="ToggleArduinoMode"/>
      <F2 action="ChangeLogLevel"/>
      </page>
	  <page model="ConfigGPSPage">
			<Accept model="NavitCommonConfigPage" caption="Go to Navit Config"/>
			<Cancel model="CommonConfigPage" view="CommonVertcalStackPage" caption="Back"/>
			<F1 action="ToggleGPSDaemon" />
			<F2 model="NavitCommonConfigPage" view="CommonVertcalStackPage" caption="Navit common config"/>
			<F3 model="NavitOSDConfigPage" view="CommonVertcalStackPage" caption="Navit OSD config"/>
		</page>
		<page model="NavitCommonConfigPage">
			<Cancel model="ConfigGPSPage" view="CommonVertcalStackPage" caption="Back"/>
			<F1 action="ActivateGPS"/>
			<F2 action="Autozoom"/>
			<F3 action="Menubar"/>
			<F4 action="Toolbar"/>
			<F5 action="Statusbar"/>
			<F6 action="KeepNorth"/>
			<F7 action="LockRoad"/>
			<F8 action="Zoom"/>
		</page>
		<page model="NavitOSDConfigPage">
			<Cancel model="ConfigGPSPage" view="CommonVertcalStackPage" caption="Back"/>
			<F1 action=""/>
			<F2 action=""/>
			<F3 action=""/>
			<F4 action=""/>
			<F5 action=""/>
			<F6 action=""/>
		</page>
  </pages>
</app>
